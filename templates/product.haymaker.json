    "1638156556bf7ce5ca": {
      "type": "sh-julia-testimonials-1",
      "blocks": {
        "53bb65d4-c25d-47c2-baf3-1ee171436d1e": {
          "type": "testimonial",
          "settings": {
            "name": "- Nathan G",
            "title": "",
            "content": "<p><strong>Delicious & Great Service<\/strong><em><br\/><br\/>Have been purchasing from Rumble for over a year now. Service in both the online store and the store in Kensington is great, and the coffee is ace. Delicious, ethical, and consistent.<\/em><\/p>"
          }
        },
        "b860d139-7043-4d6b-8d32-fd8204a2198b": {
          "type": "testimonial",
          "settings": {
            "name": "- Brent D",
            "title": "",
            "content": "<p><strong>Great Coffee & Service<br\/><\/strong><br\/><em>I've been buying the Haymaker Blend for a few years now. Never lets me down.<\/em><\/p><p><em>Great tasting in both my long blacks and my wife's soy caps.<\/em><\/p><p><em>More often than not, the coffee I make at home is better than the coffee i buy at cafes.<\/em><\/p>"
          }
        },
        "ed247373-7d14-4a80-b3cf-134ad0f1336a": {
          "type": "testimonial",
          "settings": {
            "name": "- Rick W",
            "title": "",
            "content": "<p><strong>Delicious!<\/strong><\/p><p><em><br\/>Delicious as always from Rumble, love the transparency initiative love the coffee. Making my espresso on the Flair works a treat.<\/em><\/p><p><\/p>"
          }
        }
      },
